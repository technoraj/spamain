{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "belgiumSpaApp": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/src/assets"
              },
              {
                "glob": "**/*",
                "input": "src/environments",
                "output": "/src/environments"
              },
              "src/web.config"
            ],
            "styles": [
              "src/styles.css",
              "./node_modules/bootstrap/dist/css/bootstrap.css",
              "./node_modules/font-awesome/css/font-awesome.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/bootstrap-datepicker.min.css",
              "src/assets/css/font-awesome.min.css",
              "src/assets/css/xs-icon.css",
              "src/assets/css/owl.carousel.min.css",
              "src/assets/css/themeSwither.css",
              "src/assets/css/isotope.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/plugins.css",
              "src/assets/css/style.css",
              "src/assets/css/style2.css",
              "src/assets/css/responsive.css",
              "./node_modules/primeng/resources/themes/nova/theme.css",
              "./node_modules/primeng/resources/primeng.min.css"
            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.min.js",
              "./node_modules/popper.js/dist/umd/popper.min.js",
              "./node_modules/bootstrap/dist/js/bootstrap.js",
              "src/assets/js/plugins.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/bootstrap-datepicker.min.js",
              "src/assets/js/isotope.pkgd.min.js",
              "src/assets/js/jquery.ajaxchimp.min.js",
              "src/assets/js/owl.carousel.min.js",
              "src/assets/js/jquery.magnific-popup.min.js",
              "src/assets/js/appear.js",
              "src/assets/js/spectragram.min.js",
              "src/assets/js/themeSwither.js",
              "src/assets/js/main.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "belgiumSpaApp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "belgiumSpaApp:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "belgiumSpaApp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "node_modules/primeng/resources/themes/nova/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/font-awesome/css/font-awesome.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "belgiumSpaApp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "belgiumSpaApp:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "belgiumSpaApp",
  "cli": {
    "analytics": "21a89504-f8e1-4b60-a308-d1ecaa1dc195"
  }
}